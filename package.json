{
  "name": "training-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "frontend": "yarn workspace @training-project/frontend",
    "backend": "yarn workspace @training-project/backend",
    "start": "concurrently \"yarn frontend start\" \"yarn backend start\""
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.3",
    "axios": "^0.27.2",
    "concurrently": "^5.3.0",
    "react-hook-form": "^7.34.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0"
  },
  "workspaces": [
    "backend",
    "frontend"
  ],
  "prettier": {
    "printWidth": 120,
    "useTabs": false,
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "es5",
    "arrowParens": "always"
  }
}
